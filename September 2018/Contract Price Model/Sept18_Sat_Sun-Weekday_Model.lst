 162  *-------------------------------------------------------------------------
      -----*
 163   
 164  *Mass Balance Equation
 165  EQ1_ResMassBal..                               storage =e= Initstorage + (
      Inflow*Convert*sum(p,Duration(p))*Totaldays)- Released_vol - evap;
 166  *                                                                         
       CFS * conversion factor from cfs to ac-ft/hr and multiplied by 24 (total 
      duration in a day) then multiplied by total day in the month
 167  *=======================Physical Constraints==============================
      =======
 168  EQ2_reqpowerstorage..                          storage =g= minstorage;
 169  EQ3_maxstor..                                  storage =l= maxstorage;
 170   
 171  EQ4_MaxR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=l= maxRel;
 172   
 173  EQ5_MinR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=g= minRel;
 174   
 175  EQ6_ZeroFlowDays(FlowPattern,d,p)$(Num_Days(FlowPattern,d) eq 0)..        
           Release(FlowPattern,d,p)=e=0;
 176   
 177  EQ7_Rel_Range(FlowPattern,d)$(Num_Days(FlowPattern,d) gt 0)..             
           Release(FlowPattern,d,"pHigh")- Release(FlowPattern,d,"pLow")=l=Daily
      _RelRange;
 178   
 179  *EQ8_  constraining the overall monthly released volume..
 180  EQ8_Monthtlyrel..                                                         
           TotMonth_volume=e= Released_vol;
 181   
 182  EQ9_RelVolume..                                                           
           Released_vol=e= sum(FlowPattern,sum(d, sum(p, Release(FlowPattern,d,p
      )*Convert*Duration(p))*Num_Days(FlowPattern,d)));
 183   
 184  *======================Managerial Constraints=============================
      ======================
 185   
 186  EQ10_SteadyFlow_Day(FlowPattern,d)$(Num_Days("Steady",d) gt 0)..          
           Release("Steady",d,"pHigh") =e= Release("Steady",d,"pLow");
 187   
 188  EQ11_Unsteadydays_OffpeakRel(FlowPattern,d)$(Num_Days("Unsteady",d) gt 0).
      .    Release("Unsteady",d,"pLow")=e= Release("Unsteady","Weekday","pLow");
 189   
 190   
 191  EQ12_OffsetRel(FlowPattern)$((Num_days("Unsteady","Weekday") gt 0) and (Nu
      m_Days("Steady","Sunday") gt 0))..          Release("Steady","Sunday","pLo
      w")=e= Release("Unsteady","Weekday","pLow")+ Weekend_Rel;
 192  * EQ12_  finds the minimimum release value from the hydrograph plus additi
      onal release we desire on weekends above off-peak weekday release (i.e. Of
      fset).
 193   
 194  EQ13a_OnPeak_Saturday$(Num_Days("Unsteady","Saturday") gt 0)..          Re
      lease("Unsteady","Saturday","pHigh")=e= Release("Unsteady","Weekday","pHig
      h")-2000;
 195  EQ13b_OnPeak_Sunday$((Num_Days("Unsteady","Sunday") gt 0))..            Re
      lease("Unsteady","Sunday","pHigh")=e= Release("Unsteady","Weekday","pHigh"
      )-2000;
 196   
 197  EQ14_Steady_Saturdays(FlowPattern,d,p)$(Num_Days("Steady","Saturday")gt 0)
      ..   Release("Steady","Saturday",p)=e= Release("Steady","Sunday",p);
 198   
 199  EQ15_Steady_Weekdays(FlowPattern,d,p)$(Num_Days("Steady","Weekday")gt 0)..
           Release("Steady","Weekday",p)=e= Release("Steady","Sunday",p);
 200   
 201  *EQ16_Steady_Weekdays(FlowPattern,d,p)$((Num_Days("Steady","Weekday")gt 0)
      and(Num_Days("Unsteady","Weekday")gt 0))..     Release("Steady","Weekday",
      p)=e= Release("Steady","Sunday",p)- Weekend_Rel ;
 202   
 203   
 204  EQ17_OnPeak_UnsteadyDay(FlowPattern,d)$(Num_Days("Unsteady",d)gt 0)..     
           Release("Unsteady",d,"pHigh")=g= Release("Unsteady",d,"pLow");
 205   
 206  *==============================================================
 207  *EQ18a_OnPeakUnsteady_Sat(FlowPattern,d)$((Num_Days("Unsteady","Saturday")
      gt 0)and (Num_Days("Steady","Sunday") gt 0))..                  Release("U
      nsteady","Saturday","pHigh")=g= 11000;
 208  *EQ18b_OnPeakUnsteady_Sun(FlowPattern,d)$((Num_Days("Unsteady","Sunday")gt
       0)and (Num_Days("Steady","Sunday") gt 0))..                    Release("U
      nsteady","Sunday","pHigh")=g= 10000;
 209  *==============================================================
 210   
 211  EQ19_EnergyGen_Max(FlowPattern,d,p)..                                     
           Energy_Gen(FlowPattern,d,p)=l= 1320*Duration(p);
 212  *Maximum Energy Generation capacity of GCD (MWH).. Source https://www.usbr
      .gov/uc/rm/crsp/gc
 213  EQ20_EnergyGen(FlowPattern,d,p)..                                         
           Energy_Gen(FlowPattern,d,p)=e= Release(FlowPattern,d,p)*Duration(p)*0
      .03715;
 214  * Energy generation formula used in WAPA Execl model..
 215   
 216  *==============================Objective Function=========================
      =============
 217  EQ21_EnergyRevenue..                                                      
           ObjectiveVal=e= sum(FlowPattern,sum(d, sum(p,Energy_Gen(FlowPattern,d
      ,p)*Energy_Price(d,p))*Num_Days(FlowPattern,d)));
 218  *-------------------------------------------------------------------------
      -----*
 219   
 220   
 221   
 222  MODEL Model1 Find value of hydropower revenue using LP/ALL/;
 223  *This model is for all cases of steady flow days
 224   
 225  Loop((Offset,tot_vol,Nu_SteadyDays),
 226   
 227  Weekend_Rel= Diff_Release(Offset);
 228  TotMonth_volume = Vol_monthlyrelease(tot_vol);
 229  Num_Days(FlowType,Days)= Days_Distribution(FlowType,Days,Nu_SteadyDays)+ E
      PS;
 230  Energy_Price(Days,p)= Energy_Rate(Days,p);
 231   
 232  option LP= CPLEX;
 233   
 234  ******************************
 235  *initialization (Its optional in linear progromming but required in non-li
      near programming)
 236  *Release.L(FlowPattern,d,p)= 1000;
 237  **********************************
 238   
 239  Solve Model1 using LP MAXIMIZE ObjectiveVal;
 240   
 241  *-------------------------------------------------------------------------
      ---------------------------------------------
 242  *-------------------------------------------------------------------------
      ---------------------------------------------
 243  * All the following lines of code are saving values for different paramete
      rs
 244   
 245     FStore(Offset,tot_vol,Nu_SteadyDays)= ObjectiveVal.L;
 246   
 247  * XStore store the energy generated (Mwh/day) during different types of da
      ys
 248     XStore(Offset,tot_vol,Nu_SteadyDays,FlowType,Days)= sum (p,Energy_Gen.L
      (FlowType,Days,p))+ EPS;
 249   
 250  * RStore store the reservoir releases (cfs) during different types of days
       and scenarios.
 251     RStore(Offset,tot_vol,Nu_SteadyDays,FlowType,Days,p)= Release.L(FlowTyp
      e,Days,p)+ EPS;
 252   
 253  *Sstore store the end of month reservoir storage (ac-ft)
 254     Sstore(Offset,tot_vol,Nu_SteadyDays)= storage.L;
 255   
 256   
 257  *Saving the model status for different scenarios.
 258     ModelResults(Offset,tot_vol,Nu_SteadyDays,"SolStat")= Model1.solvestat;
 259     ModelResults(Offset,tot_vol,Nu_SteadyDays,"ModStat")= Model1.modelstat;
 260   
 261   
 262     option clear=ObjectiveVal,clear=Release,clear=Energy_Gen;
 263  );
 264   
 265  DISPLAY FStore,XStore,RStore,Sstore;
