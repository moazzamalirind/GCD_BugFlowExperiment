 271  *==============================Objective Function=========================
      =============
 272   
 273  EQ22_SurplusDeficitEnergy(flowpattern, d, p)$(Num_Days(flowpattern,d) gt 0
      )..    Revenue(flowpattern,d,p) =e= {MinimumRelease(flowpattern,d,p) * Ene
      rgy_Price(d,"Contract",p) + (Release(flowpattern,d,p) - Observed_Rel(d,p))
      *Energy_Price(d,"Market",p)}*0.03715*Duration(p)* Num_Days(flowpattern,d);
 274  * Set revenue to zero for flowpatterns that have zero days
 275  EQ23_ZeroDays(flowpattern,d,p)$(Num_Days(flowpattern,d) eq 0)..           
             Revenue(flowpattern,d,p) =e= 0;
 276   
 277  EQ24_MinimumFlow_Release(flowpattern, d, p)$(Num_Days(flowpattern,d) gt 0)
      ..     MinimumRelease(flowpattern,d,p) =L= Release(flowpattern,d,p);
 278  EQ25_MinimumFlow_Observed(flowpattern, d, p)$(Num_Days(flowpattern,d) gt 0
      )..    MinimumRelease(flowpattern,d,p) =L= Observed_Rel(d,p);
 279   
 280  EQ26_TotalRevenue..                                                       
             ObjectiveVal=e= Sum((FlowPattern,d,p),Revenue(FlowPattern,d,p));
