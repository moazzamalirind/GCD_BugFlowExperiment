 217  *Mass Balance Equation
 218  EQ1_ResMassBal..                               storage =e= Initstorage + (
      Inflow*Convert*sum(p,Duration(p))*Totaldays)- Released_vol - evap;
 219  *                                                                         
       CFS * conversion factor from cfs to ac-ft/hr and multiplied by 24 (total 
      duration in a day) then multiplied by total day in the month
 220  *=======================Physical Constraints==============================
      =======
 221  EQ2_reqpowerstorage..                          storage =g= minstorage;
 222  EQ3_maxstor..                                  storage =l= maxstorage;
 223   
 224  EQ4_MaxR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=l= maxRel;
 225   
 226  EQ5_MinR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=g= minRel;
 227   
 228  EQ6_ZeroFlowDays(FlowPattern,d,p)$(Num_Days(FlowPattern,d) eq 0)..        
           Release(FlowPattern,d,p)=e=0;
 229   
 230  EQ7_Rel_Range(FlowPattern,d)$(Num_Days(FlowPattern,d) gt 0)..             
           Release(FlowPattern,d,"pHigh")- Release(FlowPattern,d,"pLow")=l=Daily
      _RelRange;
 231   
 232  EQ8_AlternativeDays_Relrange$(Num_days("HydroPeak","Sunday") gt 0)..      
           Release("HydroPeak","Saturday","pHigh")- Release("HydroPeak","Sunday"
      ,"pLow")=l=Daily_RelRange;
 233  EQ9_AlternativeDays_Relrange$(Num_days("HydroPeak","Saturday") gt 0) ..   
           Release("HydroPeak","Weekday","pHigh")- Release("HydroPeak","Saturday
      ","pLow")=l=Daily_RelRange;
 234  EQ10_AlternativeDays_Relrange..                                           
           Release("HydroPeak","Sunday","pHigh")- Release("HydroPeak","Weekday",
      "pLow")=l=Daily_RelRange;
