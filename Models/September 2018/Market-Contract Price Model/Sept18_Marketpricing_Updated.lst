 270  *==============================Objective Function=========================
      =============
 271   
 272  EQ22_SurplusDeficitEnergy(flowpattern, d, p)$(Num_Days(flowpattern,d) gt 0
      )..    Revenue(flowpattern,d,p) =e= {MinimumRelease(flowpattern,d,p) * Ene
      rgy_Price(d,"Contract",p) + (Release(flowpattern,d,p) - Observed_Rel(d,p))
      *Energy_Price(d,"Market",p)}*0.03715*Duration(p)* Num_Days(flowpattern,d);
 273  * Set revenue to zero for flowpatterns that have zero days
 274  EQ23_ZeroDays(flowpattern,d,p)$(Num_Days(flowpattern,d) eq 0)..           
             Revenue(flowpattern,d,p) =e= 0;
 275   
 276  EQ24_MinimumFlow_Release(flowpattern, d, p)$(Num_Days(flowpattern,d) gt 0)
      ..     MinimumRelease(flowpattern,d,p) =L= Release(flowpattern,d,p);
 277  EQ25_MinimumFlow_Observed(flowpattern, d, p)$(Num_Days(flowpattern,d) gt 0
      )..    MinimumRelease(flowpattern,d,p) =L= Observed_Rel(d,p);
 278   
 279  EQ26_TotalRevenue..                                                       
             ObjectiveVal=e= Sum((FlowPattern,d,p),Revenue(FlowPattern,d,p));
