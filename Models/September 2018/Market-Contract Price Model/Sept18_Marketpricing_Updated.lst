 218  *Mass Balance Equation
 219  EQ1_ResMassBal..                               storage =e= Initstorage + (
      Inflow*Convert*sum(p,Duration(p))*Totaldays)- Released_vol - evap;
 220  *                                                                         
       CFS * conversion factor from cfs to ac-ft/hr and multiplied by 24 (total 
      duration in a day) then multiplied by total day in the month
 221  *=======================Physical Constraints==============================
      =======
 222  EQ2_reqpowerstorage..                          storage =g= minstorage;
 223  EQ3_maxstor..                                  storage =l= maxstorage;
 224   
 225  EQ4_MaxR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=l= maxRel;
 226   
 227  EQ5_MinR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=g= minRel;
 228   
 229  EQ6_ZeroFlowDays(FlowPattern,d,p)$(Num_Days(FlowPattern,d) eq 0)..        
           Release(FlowPattern,d,p)=e=0;
 230   
 231  EQ7_Rel_Range(FlowPattern,d)$(Num_Days(FlowPattern,d) gt 0)..             
           Release(FlowPattern,d,"pHigh")- Release(FlowPattern,d,"pLow")=l=Daily
      _RelRange;
 232   
 233  EQ8_AlternativeDays_Relrange$(Num_days("HydroPeak","Sunday") gt 0)..      
           Release("HydroPeak","Saturday","pHigh")- Release("HydroPeak","Sunday"
      ,"pLow")=l=Daily_RelRange;
 234  EQ9_AlternativeDays_Relrange$(Num_days("HydroPeak","Saturday") gt 0) ..   
           Release("HydroPeak","Weekday","pHigh")- Release("HydroPeak","Saturday
      ","pLow")=l=Daily_RelRange;
 235  EQ10_AlternativeDays_Relrange$(Num_days("HydroPeak","Sunday") gt 0)..     
           Release("HydroPeak","Sunday","pHigh")- Release("HydroPeak","Weekday",
      "pLow")=l=Daily_RelRange;
 236   
