 286  EQ24_MinimumFlow_Release(flowpattern, d, p)$(Num_Days(flowpattern,d) gt 0)
      ..     MinimumRelease(flowpattern,d,p) =L= Release(flowpattern,d,p);
 287  EQ25_MinimumFlow_Observed(flowpattern, d, p)$(Num_Days(flowpattern,d) gt 0
      )..    MinimumRelease(flowpattern,d,p) =L= Observed_Rel(d,p);
 288   
 289  EQ26_TotalRevenue..                                                       
             ObjectiveVal=e= Sum((FlowPattern,d,p),Revenue(FlowPattern,d,p));
