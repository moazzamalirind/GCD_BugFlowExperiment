 215  *Mass Balance Equation
 216  EQ1_ResMassBal..                               storage =e= Initstorage + (
      Inflow*Convert*sum(p,Duration(p))*Totaldays)- Released_vol - evap;
 217  *                                                                         
       CFS * conversion factor from cfs to ac-ft/hr and multiplied by 24 (total 
      duration in a day) then multiplied by total day in the month
 218  *=======================Physical Constraints==============================
      =======
 219  EQ2_reqpowerstorage..                          storage =g= minstorage;
 220  EQ3_maxstor..                                  storage =l= maxstorage;
 221   
 222  EQ4_MaxR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=l= maxRel;
 223   
 224  EQ5_MinR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=g= minRel;
 225   
 226  EQ6_ZeroFlowDays(FlowPattern,d,p)$(Num_Days(FlowPattern,d) eq 0)..        
           Release(FlowPattern,d,p)=e=0;
 227   
 228  EQ7_Rel_Range(FlowPattern,d)$(Num_Days(FlowPattern,d) gt 0)..             
           Release(FlowPattern,d,"pHigh")- Release(FlowPattern,d,"pLow")=l=Daily
      _RelRange;
 229   
 230  EQ8_AlternativeDays_Relrange$(Num_days("HydroPeak","Sunday") gt 0)..      
           Release("HydroPeak","Saturday","pHigh")- Release("HydroPeak","Sunday"
      ,"pLow")=l=Daily_RelRange;
 231  EQ9_AlternativeDays_Relrange$(Num_days("HydroPeak","Saturday") gt 0) ..   
           Release("HydroPeak","Weekday","pHigh")- Release("HydroPeak","Saturday
      ","pLow")=l=Daily_RelRange;
 232  EQ10_AlternativeDays_Relrange..                                           
           Release("HydroPeak","Sunday","pHigh")- Release("HydroPeak","Weekday",
      "pLow")=l=Daily_RelRange;
