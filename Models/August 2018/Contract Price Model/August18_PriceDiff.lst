 183  *-------------------------------------------------------------------------
      -----*
 184   
 185  *Mass Balance Equation
 186  EQ1_ResMassBal..                               storage =e= Initstorage + (
      Inflow*Convert*sum(p,Duration(p))*Totaldays)- Released_vol - evap;
 187  *                                                                         
       CFS * conversion factor from cfs to ac-ft/hr and multiplied by 24 (total 
      duration in a day) then multiplied by total day in the month
 188  *=======================Physical Constraints==============================
      =======
 189  EQ2_reqpowerstorage..                          storage =g= minstorage;
 190  EQ3_maxstor..                                  storage =l= maxstorage;
 191   
 192  EQ4_MaxR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=l= maxRel;
 193   
 194  EQ5_MinR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=g= minRel;
 195   
 196  EQ6_ZeroFlowDays(FlowPattern,d,p)$(Num_Days(FlowPattern,d) eq 0)..        
           Release(FlowPattern,d,p)=e=0;
 197   
 198  EQ7_Rel_Range(FlowPattern,d)$(Num_Days(FlowPattern,d) gt 0)..             
           Release(FlowPattern,d,"pHigh")- Release(FlowPattern,d,"pLow")=l=Daily
      _RelRange;
 199   
 200  *EQ8_  constraining the overall monthly released volume..
 201  EQ8_Monthtlyrel..                                                         
           TotMonth_volume=e= Released_vol;
 202   
 203  EQ9_RelVolume..                                                           
           Released_vol=e= sum(FlowPattern,sum(d, sum(p, Release(FlowPattern,d,p
      )*Convert*Duration(p))*Num_Days(FlowPattern,d)));
 204   
 205  *======================Managerial Constraints=============================
      ======================
 206   
 207  EQ10_SteadyFlow_Day(FlowPattern,d)$(Num_Days("Steady",d) gt 0)..          
           Release("Steady",d,"pHigh") =e= Release("Steady",d,"pLow");
 208   
 209  EQ11_Unsteadydays_OffpeakRel(FlowPattern,d)$(Num_Days("Unsteady",d) gt 0).
      .    Release("Unsteady",d,"pLow")=e= Release("Unsteady","Weekday","pLow");
 210   
 211  EQ12_OffsetRel(FlowPattern)$((Num_days("Unsteady","Weekday") gt 0) and (Nu
      m_Days("Steady","Sunday") gt 0))..          Release("Steady","Sunday","pLo
      w")=e= Release("Unsteady","Weekday","pLow")+ Weekend_Rel;
 212  * EQ12_  finds the minimimum release value from the hydrograph plus additi
      onal release we desire on weekends above off-peak weekday release (i.e. Of
      fset).
 213   
 214   
 215  EQ13a_OnPeak_Saturday$(Num_Days("Unsteady","Saturday") gt 0)..          Re
      lease("Unsteady","Saturday","pHigh")=e= Release("Unsteady","Weekday","pHig
      h")-2000;
 216  EQ13b_OnPeak_Sunday$((Num_Days("Unsteady","Sunday") gt 0))..            Re
      lease("Unsteady","Sunday","pHigh")=e= Release("Unsteady","Weekday","pHigh"
      )-2000;
 217   
 218   
 219  EQ14_Steady_Saturdays(FlowPattern,d,p)$(Num_Days("Steady","Saturday")gt 0)
      ..   Release("Steady","Saturday",p)=e= Release("Steady","Sunday",p);
 220   
 221  EQ15_Steady_Weekdays(FlowPattern,d,p)$(Num_Days("Steady","Weekday")gt 0)..
           Release("Steady","Weekday",p)=e= Release("Steady","Sunday",p);
 222   
 223  *EQ16_Steady_Weekdays(FlowPattern,d,p)$((Num_Days("Steady","Weekday")gt 0)
      and(Num_Days("Unsteady","Weekday")gt 0))..     Release("Steady","Weekday",
      p)=e= Release("Steady","Sunday",p)- Weekend_Rel ;
 224   
 225   
 226  EQ17_OnPeak_UnsteadyDay(FlowPattern,d)$(Num_Days("Unsteady",d)gt 0)..     
           Release("Unsteady",d,"pHigh")=g= Release("Unsteady",d,"pLow");
 227   
 228  EQ19_EnergyGen_Max(FlowPattern,d,p)..                                     
           Energy_Gen(FlowPattern,d,p)=l= 1320*Duration(p);
 229  *Maximum Energy Generation capacity of GCD (MWH).. Source https://www.usbr
      .gov/uc/rm/crsp/gc
 230  EQ20_EnergyGen(FlowPattern,d,p)..                                         
           Energy_Gen(FlowPattern,d,p)=e= Release(FlowPattern,d,p)*Duration(p)*0
      .03715;
 231  * Energy generation formula used in WAPA Execl model..
 232   
 233  *==============================Objective Function=========================
      =============
 234  EQ21_EnergyRevenue..                                                      
           ObjectiveVal=e= sum(FlowPattern,sum(d, sum(p,Energy_Gen(FlowPattern,d
      ,p)*Energy_Price(d,p))*Num_Days(FlowPattern,d)));
 235  *-------------------------------------------------------------------------
      -----*
 236   
 237   
 238   
 239  MODEL Model1 Find value of hydropower revenue using LP/ALL/;
 240  *This model is for all cases of steady flow days
 241   
 242  Loop((Offset,tot_vol,Nu_SteadyDays),
 243   
 244  Weekend_Rel= Diff_Release(Offset);
 245  TotMonth_volume = Vol_monthlyrelease(tot_vol);
 246  Num_Days(FlowType,Days)= Days_Distribution(FlowType,Days,Nu_SteadyDays)+ E
      PS;
 247  Energy_Price(Days,p)= Energy_Rate(Days,p);
 248   
 249  option LP= CPLEX;
 250   
 251  ******************************
 252  *initialization (Its optional in linear progromming but required in non-li
      near programming)
 253  *Release.L(FlowPattern,d,p)= 1000;
 254  **********************************
 255   
 256  Solve Model1 using LP MAXIMIZE ObjectiveVal;
 257   
 258  *-------------------------------------------------------------------------
      ---------------------------------------------
 259  *-------------------------------------------------------------------------
      ---------------------------------------------
 260  * All the following lines of code are saving values for different paramete
      rs
 261   
 262     FStore(Offset,tot_vol,Nu_SteadyDays)= ObjectiveVal.L;
 263   
 264  * XStore store the energy generated (Mwh/day) during different types of da
      ys
 265     XStore(Offset,tot_vol,Nu_SteadyDays,FlowType,Days)= sum (p,Energy_Gen.L
      (FlowType,Days,p))+ EPS;
 266   
 267  * RStore store the reservoir releases (cfs) during different types of days
       and scenarios.
 268     RStore(Offset,tot_vol,Nu_SteadyDays,FlowType,Days,p)= Release.L(FlowTyp
      e,Days,p)+ EPS;
 269   
 270  *Sstore store the end of month reservoir storage (ac-ft)
 271     Sstore(Offset,tot_vol,Nu_SteadyDays)= storage.L;
 272   
 273   
 274  *Saving the model status for different scenarios.
 275     ModelResults(Offset,tot_vol,Nu_SteadyDays,"SolStat")= Model1.solvestat;
 276     ModelResults(Offset,tot_vol,Nu_SteadyDays,"ModStat")= Model1.modelstat;
 277   
 278   
 279     option clear=ObjectiveVal,clear=Release,clear=Energy_Gen;
 280  );
 281   
 282  DISPLAY FStore,XStore,RStore,Sstore;
