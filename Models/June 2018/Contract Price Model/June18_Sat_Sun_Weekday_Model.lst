 158   
 159  *Mass Balance Equation
 160  EQ1_ResMassBal..                               storage =e= Initstorage + (
      Inflow*Convert*sum(p,Duration(p))*Totaldays)- Released_vol - evap;
 161  *                                                                         
       CFS * conversion factor from cfs to ac-ft/hr and multiplied by 24 (total 
      duration in a day) then multiplied by total day in the month
 162  *=======================Physical Constraints==============================
      =======
 163  EQ2_reqpowerstorage..                          storage =g= minstorage;
 164  EQ3_maxstor..                                  storage =l= maxstorage;
 165   
 166  EQ4_MaxR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=l= maxRel;
 167   
 168  EQ5_MinR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=g= minRel;
 169   
 170  EQ6_ZeroFlowDays(FlowPattern,d,p)$(Num_Days(FlowPattern,d) eq 0)..        
           Release(FlowPattern,d,p)=e=0;
 171   
 172  EQ7_Rel_Range(FlowPattern,d)$(Num_Days(FlowPattern,d) gt 0)..             
           Release(FlowPattern,d,"pHigh")- Release(FlowPattern,d,"pLow")=l=Daily
      _RelRange;
 173   
 174  *EQ8_  constraining the overall monthly released volume..
 175  EQ8_Monthtlyrel..                                                         
           TotMonth_volume=e= Released_vol;
 176   
 177  EQ9_RelVolume..                                                           
           Released_vol=e= sum(FlowPattern,sum(d, sum(p, Release(FlowPattern,d,p
      )*Convert*Duration(p))*Num_Days(FlowPattern,d)));
 178   
 179  *======================Managerial Constraints=============================
      ======================
 180   
 181  EQ10_SteadyFlow_Day(FlowPattern,d)$(Num_Days("Steady",d) gt 0)..          
           Release("Steady",d,"pHigh") =e= Release("Steady",d,"pLow");
 182   
 183  EQ11_Unsteadydays_OffpeakRel(FlowPattern,d)$(Num_Days("Unsteady",d) gt 0).
      .    Release("Unsteady",d,"pLow")=e= Release("Unsteady","Weekday","pLow");
 184   
 185   
 186  EQ12_OffsetRel(FlowPattern)$((Num_days("Unsteady","Weekday") gt 0) and (Nu
      m_Days("Steady","Sunday") gt 0))..          Release("Steady","Sunday","pLo
      w")=e= Release("Unsteady","Weekday","pLow")+ Weekend_Rel;
 187  * EQ12_  finds the minimimum release value from the hydrograph plus additi
      onal release we desire on weekends above off-peak weekday release (i.e. Of
      fset).
 188   
 189  EQ13a_OnPeak_Saturday$(Num_Days("Unsteady","Saturday") gt 0)..          Re
      lease("Unsteady","Saturday","pHigh")=e= Release("Unsteady","Weekday","pHig
      h")-2000;
 190  EQ13b_OnPeak_Sunday$((Num_Days("Unsteady","Sunday") gt 0))..            Re
      lease("Unsteady","Sunday","pHigh")=e= Release("Unsteady","Weekday","pHigh"
      )-2000;
 191   
 192  EQ14_Steady_Saturdays(FlowPattern,d,p)$(Num_Days("Steady","Saturday")gt 0)
      ..   Release("Steady","Saturday",p)=e= Release("Steady","Sunday",p);
 193   
 194  EQ15_Steady_Weekdays(FlowPattern,d,p)$(Num_Days("Steady","Weekday")gt 0)..
           Release("Steady","Weekday",p)=e= Release("Steady","Sunday",p);
 195   
 196  *EQ16_Steady_Weekdays(FlowPattern,d,p)$((Num_Days("Steady","Weekday")gt 0)
      and(Num_Days("Unsteady","Weekday")gt 0))..     Release("Steady","Weekday",
      p)=e= Release("Steady","Sunday",p)- Weekend_Rel ;
 197   
 198   
 199  EQ17_OnPeak_UnsteadyDay(FlowPattern,d)$(Num_Days("Unsteady",d)gt 0)..     
           Release("Unsteady",d,"pHigh")=g= Release("Unsteady",d,"pLow");
 200   
 201  *==============================================================
 202  *EQ18a_OnPeakUnsteady_Sat(FlowPattern,d)$((Num_Days("Unsteady","Saturday")
      gt 0)and (Num_Days("Steady","Sunday") gt 0))..                  Release("U
      nsteady","Saturday","pHigh")=g= 11000;
 203  *EQ18b_OnPeakUnsteady_Sun(FlowPattern,d)$((Num_Days("Unsteady","Sunday")gt
       0)and (Num_Days("Steady","Sunday") gt 0))..                    Release("U
      nsteady","Sunday","pHigh")=g= 10000;
 204  *==============================================================
 205   
 206  EQ19_EnergyGen_Max(FlowPattern,d,p)..                                     
           Energy_Gen(FlowPattern,d,p)=l= 1320*Duration(p);
 207  *Maximum Energy Generation capacity of GCD (MWH).. Source https://www.usbr
      .gov/uc/rm/crsp/gc
 208  EQ20_EnergyGen(FlowPattern,d,p)..                                         
           Energy_Gen(FlowPattern,d,p)=e= Release(FlowPattern,d,p)*Duration(p)*0
      .03715;
 209  * Energy generation formula used in WAPA Execl model..
 210   
 211  *==============================Objective Function=========================
      =============
 212  EQ21_EnergyRevenue..                                                      
           ObjectiveVal=e= sum(FlowPattern,sum(d, sum(p,Energy_Gen(FlowPattern,d
      ,p)*Energy_Price(d,p))*Num_Days(FlowPattern,d)));
 213  *-------------------------------------------------------------------------
      -----*
 214   
 215   
 216   
 217  MODEL Model1 Find value of hydropower revenue using LP/ALL/;
 218  *This model is for all cases of steady flow days
 219   
 220  Loop((Offset,tot_vol,Nu_SteadyDays),
 221   
 222  Weekend_Rel= Diff_Release(Offset);
 223  TotMonth_volume = Vol_monthlyrelease(tot_vol);
 224  Num_Days(FlowType,Days)= Days_Distribution(FlowType,Days,Nu_SteadyDays)+ E
      PS;
 225  Energy_Price(Days,p)= Energy_Rate(Days,p);
 226   
 227  option LP= CPLEX;
 228   
 229  ******************************
 230  *initialization (Its optional in linear progromming but required in non-li
      near programming)
 231  *Release.L(FlowPattern,d,p)= 1000;
 232  **********************************
 233   
 234  Solve Model1 using LP MAXIMIZE ObjectiveVal;
 235   
 236  *-------------------------------------------------------------------------
      ---------------------------------------------
 237  *-------------------------------------------------------------------------
      ---------------------------------------------
 238  * All the following lines of code are saving values for different paramete
      rs
 239   
 240     FStore(Offset,tot_vol,Nu_SteadyDays)= ObjectiveVal.L;
 241   
 242  * XStore store the energy generated (Mwh/day) during different types of da
      ys
 243     XStore(Offset,tot_vol,Nu_SteadyDays,FlowType,Days)= sum (p,Energy_Gen.L
      (FlowType,Days,p))+ EPS;
 244   
 245  * RStore store the reservoir releases (cfs) during different types of days
       and scenarios.
 246     RStore(Offset,tot_vol,Nu_SteadyDays,FlowType,Days,p)= Release.L(FlowTyp
      e,Days,p)+ EPS;
 247   
 248  *Sstore store the end of month reservoir storage (ac-ft)
 249     Sstore(Offset,tot_vol,Nu_SteadyDays)= storage.L;
 250   
 251   
 252  *Saving the model status for different scenarios.
 253     ModelResults(Offset,tot_vol,Nu_SteadyDays,"SolStat")= Model1.solvestat;
 254     ModelResults(Offset,tot_vol,Nu_SteadyDays,"ModStat")= Model1.modelstat;
 255   
 256   
 257     option clear=ObjectiveVal,clear=Release,clear=Energy_Gen;
 258  );
 259   
 260  DISPLAY FStore,XStore,RStore,Sstore;
