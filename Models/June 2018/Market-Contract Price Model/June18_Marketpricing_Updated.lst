 229  *Mass Balance Equation
 230  EQ1_ResMassBal..                               storage =e= Initstorage + (
      Inflow*Convert*sum(p,Duration(p))*Totaldays)- Released_vol - evap;
 231  *                                                                         
       CFS * conversion factor from cfs to ac-ft/hr and multiplied by 24 (total 
      duration in a day) then multiplied by total day in the month
 232  *=======================Physical Constraints==============================
      =======
 233  EQ2_reqpowerstorage..                          storage =g= minstorage;
 234  EQ3_maxstor..                                  storage =l= maxstorage;
 235   
 236  EQ4_MaxR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=l= maxRel;
 237   
 238  EQ5_MinR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=g= minRel;
 239   
 240  EQ6_ZeroFlowDays(FlowPattern,d,p)$(Num_Days(FlowPattern,d) eq 0)..        
           Release(FlowPattern,d,p)=e=0;
 241   
 242  EQ7_Rel_Range(FlowPattern,d)$(Num_Days(FlowPattern,d) gt 0)..             
           Release(FlowPattern,d,"pHigh")- Release(FlowPattern,d,"pLow")=l=Daily
      _RelRange;
 243   
 244  EQ8_AlternativeDays_Relrange$(Num_days("HydroPeak","Sunday") gt 0)..      
           Release("HydroPeak","Saturday","pHigh")- Release("HydroPeak","Sunday"
      ,"pLow")=l=Daily_RelRange;
 245  EQ9_AlternativeDays_Relrange$(Num_days("HydroPeak","Saturday") gt 0) ..   
           Release("HydroPeak","Weekday","pHigh")- Release("HydroPeak","Saturday
      ","pLow")=l=Daily_RelRange;
 246  EQ10_AlternativeDays_Relrange..                                           
           Release("HydroPeak","Sunday","pHigh")- Release("HydroPeak","Weekday",
      "pLow")=l=Daily_RelRange;
