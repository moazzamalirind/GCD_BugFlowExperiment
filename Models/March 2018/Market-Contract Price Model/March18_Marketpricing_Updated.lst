 228  *Mass Balance Equation
 229  EQ1_ResMassBal..                               storage =e= Initstorage + (
      Inflow*Convert*sum(p,Duration(p))*Totaldays)- Released_vol - evap;
 230  *                                                                         
       CFS * conversion factor from cfs to ac-ft/hr and multiplied by 24 (total 
      duration in a day) then multiplied by total day in the month
 231  *=======================Physical Constraints==============================
      =======
 232  EQ2_reqpowerstorage..                          storage =g= minstorage;
 233  EQ3_maxstor..                                  storage =l= maxstorage;
 234   
 235  EQ4_MaxR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=l= maxRel;
 236   
 237  EQ5_MinR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=g= minRel;
 238   
 239  EQ6_ZeroFlowDays(FlowPattern,d,p)$(Num_Days(FlowPattern,d) eq 0)..        
           Release(FlowPattern,d,p)=e=0;
 240   
 241  EQ7_Rel_Range(FlowPattern,d)$(Num_Days(FlowPattern,d) gt 0)..             
           Release(FlowPattern,d,"pHigh")- Release(FlowPattern,d,"pLow")=l=Daily
      _RelRange;
 242   
 243  EQ8_AlternativeDays_Relrange$(Num_days("HydroPeak","Sunday") gt 0)..      
           Release("HydroPeak","Saturday","pHigh")- Release("HydroPeak","Sunday"
      ,"pLow")=l=Daily_RelRange;
 244  EQ9_AlternativeDays_Relrange$(Num_days("HydroPeak","Saturday") gt 0) ..   
           Release("HydroPeak","Weekday","pHigh")- Release("HydroPeak","Saturday
      ","pLow")=l=Daily_RelRange;
 245  EQ10_AlternativeDays_Relrange..                                           
           Release("HydroPeak","Sunday","pHigh")- Release("HydroPeak","Weekday",
      "pLow")=l=Daily_RelRange;
