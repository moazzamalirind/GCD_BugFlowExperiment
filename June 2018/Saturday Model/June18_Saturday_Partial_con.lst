 235   
 236   
 237  MODEL Model1 Find value of hydropower revenue using LP/ALL/;
 238  *This model is for all cases of steady flow days
 239   
 240  Loop((tot_vol,Nu_SteadyDays),
 241   
 242  TotMonth_volume = Vol_monthlyrelease(tot_vol);
 243  Num_Days(FlowType,Days)= Days_Distribution(FlowType,Days,Nu_SteadyDays)+ E
      PS;
 244  Energy_Price(Days,p)= Energy_Rate(Days,p);
 245   
 246  option LP= CPLEX;
 247   
 248  ******************************
 249  *initialization (Its optional in linear progromming but required in non-li
      near programming)
 250  *Release.L(FlowPattern,d,p)= 1000;
 251  **********************************
 252   
 253  Solve Model1 using LP MAXIMIZE ObjectiveVal;
 254   
 255  *-------------------------------------------------------------------------
      ---------------------------------------------
 256  *-------------------------------------------------------------------------
      ---------------------------------------------
 257  * All the following lines of code are saving values for different paramete
      rs
 258   
 259     FStore(tot_vol,Nu_SteadyDays)= ObjectiveVal.L;
 260   
 261  * XStore store the energy generated (Mwh/day) during different types of da
      ys
 262     XStore(tot_vol,Nu_SteadyDays,FlowType,Days)= sum (p,Energy_Gen.L(FlowTy
      pe,Days,p))+ EPS;
 263   
 264  * RStore store the reservoir releases (cfs) during different types of days
       and scenarios.
 265     RStore(tot_vol,Nu_SteadyDays,FlowType,Days,p)= Release.L(FlowType,Days,
      p)+ EPS;
 266   
 267  *Sstore store the end of month reservoir storage (ac-ft)
 268     Sstore(tot_vol,Nu_SteadyDays)= storage.L;
 269   
 270   
 271  *Saving the model status for different scenarios.
 272     ModelResults(tot_vol,Nu_SteadyDays,"SolStat")= Model1.solvestat;
 273     ModelResults(tot_vol,Nu_SteadyDays,"ModStat")= Model1.modelstat;
 274   
 275   
 276     option clear=ObjectiveVal,clear=Release,clear=Energy_Gen;
 277  );
 278   
 279  DISPLAY FStore,XStore,RStore,Sstore;
