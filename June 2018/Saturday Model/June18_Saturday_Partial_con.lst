 165   
 166  *Mass Balance Equation
 167  EQ1_ResMassBal..                               storage =e= Initstorage + (
      Inflow*Convert*sum(p,Duration(p))*Totaldays)- Released_vol - evap;
 168  *                                                                         
       CFS * conversion factor from cfs to ac-ft/hr and multiplied by 24 (total 
      duration in a day) then multiplied by total day in the month
 169  *=======================Physical Constraints==============================
      =======
 170  EQ2_reqpowerstorage..                          storage =g= minstorage;
 171  EQ3_maxstor..                                  storage =l= maxstorage;
 172   
 173  EQ4_MaxR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=l= maxRel;
 174   
 175  EQ5_MinR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=g= minRel;
 176   
 177  EQ6_ZeroFlowDays(FlowPattern,d,p)$(Num_Days(FlowPattern,d) eq 0)..        
           Release(FlowPattern,d,p)=e=0;
 178   
 179  EQ7_Rel_Range(FlowPattern,d)$(Num_Days(FlowPattern,d) gt 0)..             
           Release(FlowPattern,d,"pHigh")- Release(FlowPattern,d,"pLow")=l=Daily
      _RelRange;
 180   
 181  EQ7a_btwDays_RelRange(FlowPattern,d)$(Num_Days("Unsteady","Sunday") gt 0) 
      ..     Release("Unsteady","Weekday","pLow")-Release("Unsteady","Sunday","p
      High")=l=Daily_RelRange;
 182   
 183  EQ7b_Rel_Change(FlowPattern,d)$((Num_Days("Steady","Saturday") gt 0) and (
      Num_days("Unsteady","Weekday") gt 0))..          Release("Steady","Sunday"
      ,"pHigh")-Release("Unsteady","Weekday","pLow")=l=Daily_RelRange;
 184   
 185  EQ7c_Rel_ChangeSat(FlowPattern,d)$(Num_Days("Unsteady","Saturday") gt 0)..
            Release("Unsteady","Weekday","pHigh")- Release("Unsteady","Saturday"
      ,"pLow")=l=Daily_RelRange;
 186   
 187  *EQ8_  constraining the overall monthly released volume..
 188  EQ8_Monthtlyrel..                                                         
           TotMonth_volume=e= Released_vol;
 189   
 190  EQ9_RelVolume..                                                           
           Released_vol=e= sum(FlowPattern,sum(d, sum(p, Release(FlowPattern,d,p
      )*Convert*Duration(p))*Num_Days(FlowPattern,d)));
 191   
 192  *======================Managerial Constraints=============================
      ======================
 193   
 194  EQ10_SteadyFlow_Day(FlowPattern,d)$(Num_Days("Steady",d) gt 0)..          
           Release("Steady",d,"pHigh") =e= Release("Steady",d,"pLow");
 195   
 196  EQ11_Unsteadydays_OffpeakRel(FlowPattern,d)$(Num_Days("Unsteady",d) gt 0).
      .    Release("Unsteady",d,"pLow")=e= Release("Unsteady","Weekday","pLow");
 197   
 198   
 199  EQ12_OffsetRel(FlowPattern)$((Num_days("Unsteady","Weekday") gt 0) and (Nu
      m_Days("Steady","Sunday") gt 0))..          Release("Steady","Sunday","pLo
      w")=e= Release("Unsteady","Weekday","pLow")+ Weekend_Rel;
 200  * EQ12_  finds the minimimum release value from the hydrograph plus additi
      onal release we desire on weekends above off-peak weekday release (i.e. Of
      fset).
 201   
 202   
 203   
 204  *EQ13_Zero_steadydays(FlowPattern,d,p)$(Num_Days("Steady","Sunday") eq 0).
      .      Release("Unsteady",d,p)=g= Release("Unsteady","Weekday",p);
 205   
 206   
 207  EQ13a_OnPeak_Saturday(FlowPattern,d,p)$(Num_Days("Unsteady","Saturday") gt
       0)..      Release("Unsteady","Saturday","pHigh")=g= Release("Unsteady","W
      eekday","pHigh")-2000;
 208   
 209  EQ13b_OnPeak_Sunday(FlowPattern,d,p)$(Num_Days("Unsteady","Sunday") gt 0).
      .       Release("Unsteady","Sunday","pHigh")=g= Release("Unsteady","Saturd
      ay","pHigh");
 210   
 211   
 212  EQ14_Steady_Saturdays(FlowPattern,d,p)$(Num_Days("Steady","Saturday")gt 0)
      ..   Release("Steady","Saturday",p)=e= Release("Steady","Sunday",p);
 213   
 214  EQ15_Steady_Weekdays(FlowPattern,d,p)$(Num_Days("Steady","Weekday")gt 0)..
           Release("Steady","Weekday",p)=e= Release("Steady","Sunday",p);
 215   
 216  *EQ16_Steady_Weekdays(FlowPattern,d,p)$((Num_Days("Steady","Weekday")gt 0)
      and(Num_Days("Unsteady","Weekday")gt 0))..     Release("Steady","Weekday",
      p)=e= Release("Steady","Sunday",p)- Weekend_Rel ;
 217   
 218   
 219  EQ17_OnPeak_UnsteadyDay(FlowPattern,d)$(Num_Days("Unsteady",d)gt 0)..     
           Release("Unsteady",d,"pHigh")=g= Release("Unsteady",d,"pLow");
 220   
 221  *==============================================================
 222  *EQ18a_OnPeakUnsteady_Sat(FlowPattern,d)$((Num_Days("Unsteady","Saturday")
      gt 0)and (Num_Days("Steady","Sunday") gt 0))..                  Release("U
      nsteady","Saturday","pHigh")=g= 11000;
 223  *EQ18b_OnPeakUnsteady_Sun(FlowPattern,d)$((Num_Days("Unsteady","Sunday")gt
       0)and (Num_Days("Steady","Sunday") gt 0))..                    Release("U
      nsteady","Sunday","pHigh")=g= 10000;
 224  *==============================================================
 225   
 226  EQ19_EnergyGen_Max(FlowPattern,d,p)..                                     
           Energy_Gen(FlowPattern,d,p)=l= 1320*Duration(p);
 227  *Maximum Energy Generation capacity of GCD (MWH).. Source https://www.usbr
      .gov/uc/rm/crsp/gc
 228  EQ20_EnergyGen(FlowPattern,d,p)..                                         
           Energy_Gen(FlowPattern,d,p)=e= Release(FlowPattern,d,p)*Duration(p)*0
      .03715;
 229  * Energy generation formula used in WAPA Execl model..
 230   
 231  *==============================Objective Function=========================
      =============
 232  EQ21_EnergyRevenue..                                                      
           ObjectiveVal=e= sum(FlowPattern,sum(d, sum(p,Energy_Gen(FlowPattern,d
      ,p)*Energy_Price(d,p))*Num_Days(FlowPattern,d)));
 233  *-------------------------------------------------------------------------
      -----*
