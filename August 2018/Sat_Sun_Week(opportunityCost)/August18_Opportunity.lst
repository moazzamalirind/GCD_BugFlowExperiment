 165   
 166  *Mass Balance Equation
 167  EQ1_ResMassBal..                               storage =e= Initstorage + (
      Inflow*Convert*sum(p,Duration(p))*Totaldays)- Released_vol - evap;
 168  *                                                                         
       CFS * conversion factor from cfs to ac-ft/hr and multiplied by 24 (total 
      duration in a day) then multiplied by total day in the month
 169  *=======================Physical Constraints==============================
      =======
 170  EQ2_reqpowerstorage..                          storage =g= minstorage;
 171  EQ3_maxstor..                                  storage =l= maxstorage;
 172   
 173  EQ4_MaxR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=l= maxRel;
 174   
 175  EQ5_MinR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=g= minRel;
 176   
 177  EQ6_ZeroFlowDays(FlowPattern,d,p)$(Num_Days(FlowPattern,d) eq 0)..        
           Release(FlowPattern,d,p)=e=0;
 178   
 179  EQ7_Rel_Range(FlowPattern,d)$(Num_Days(FlowPattern,d) gt 0)..             
           Release(FlowPattern,d,"pHigh")- Release(FlowPattern,d,"pLow")=l=Daily
      _RelRange;
 180   
 181  *EQ8_  constraining the overall monthly released volume..
 182  EQ8_Monthtlyrel..                                                         
           TotMonth_volume=e= Released_vol;
 183   
 184  EQ9_RelVolume..                                                           
           Released_vol=e= sum(FlowPattern,sum(d, sum(p, Release(FlowPattern,d,p
      )*Convert*Duration(p))*Num_Days(FlowPattern,d)));
 185   
 186  *======================Managerial Constraints=============================
      ======================
 187   
 188  EQ10_SteadyFlow_Day(FlowPattern,d)$(Num_Days("Steady",d) gt 0)..          
           Release("Steady",d,"pHigh") =e= Release("Steady",d,"pLow");
 189   
 190  *EQ11_Unsteadydays_OffpeakRel(FlowPattern,d)$(Num_Days("Unsteady",d) gt 0)
      ..    Release("Unsteady",d,"pLow")=e= Release("Unsteady","Weekday","pLow")
      ;
 191   
 192   
 193  EQ12_OffsetRel(FlowPattern)$((Num_days("Unsteady","Weekday") gt 0) and (Nu
      m_Days("Steady","Sunday") gt 0))..          Release("Steady","Sunday","pLo
      w")=e= Release("Unsteady","Weekday","pLow")+ Weekend_Rel;
 194  * EQ12_  finds the minimimum release value from the hydrograph plus additi
      onal release we desire on weekends above off-peak weekday release (i.e. Of
      fset).
 195   
 196  *EQ13_Zero_OffsetRel(FlowPattern,d,p)$(Num_Days("Steady","Sunday") eq 0)..
            Release("Unsteady",d,p)=e= Release("Unsteady","Weekday",p);
 197   
 198  EQ14_Steady_Saturdays(FlowPattern,d,p)$(Num_Days("Steady","Saturday")gt 0)
      ..   Release("Steady","Saturday",p)=e= Release("Steady","Sunday",p);
 199   
 200  EQ15_Steady_Weekdays(FlowPattern,d,p)$(Num_Days("Steady","Weekday")gt 0)..
           Release("Steady","Weekday",p)=e= Release("Steady","Sunday",p);
 201   
 202  *EQ16_Steady_Weekdays(FlowPattern,d,p)$((Num_Days("Steady","Weekday")gt 0)
      and(Num_Days("Unsteady","Weekday")gt 0))..     Release("Steady","Weekday",
      p)=e= Release("Steady","Sunday",p)- Weekend_Rel ;
 203   
 204   
 205  EQ17_OnPeak_UnsteadyDay(FlowPattern,d)$(Num_Days("Unsteady",d)gt 0)..     
           Release("Unsteady",d,"pHigh")=g= Release("Unsteady",d,"pLow");
 206   
 207  *==============================================================
 208  *EQ18a_OnPeakUnsteady_Sat(FlowPattern,d)$((Num_Days("Unsteady","Saturday")
      gt 0)and (Num_Days("Steady","Sunday") gt 0))..                  Release("U
      nsteady","Saturday","pHigh")=g= 11000;
 209  *EQ18b_OnPeakUnsteady_Sun(FlowPattern,d)$((Num_Days("Unsteady","Sunday")gt
       0)and (Num_Days("Steady","Sunday") gt 0))..                    Release("U
      nsteady","Sunday","pHigh")=g= 10000;
 210  *==============================================================
 211   
 212  EQ19_EnergyGen_Max(FlowPattern,d,p)..                                     
           Energy_Gen(FlowPattern,d,p)=l= 1320*Duration(p);
 213  *Maximum Energy Generation capacity of GCD (MWH).. Source https://www.usbr
      .gov/uc/rm/crsp/gc
 214  EQ20_EnergyGen(FlowPattern,d,p)..                                         
           Energy_Gen(FlowPattern,d,p)=e= Release(FlowPattern,d,p)*Duration(p)*0
      .03715;
 215  * Energy generation formula used in WAPA Execl model..
 216   
 217  *==============================Objective Function=========================
      =============
 218  EQ21_EnergyRevenue..                                                      
           ObjectiveVal=e= sum(FlowPattern,sum(d, sum(p,Energy_Gen(FlowPattern,d
      ,p)*Energy_Price(d,p))*Num_Days(FlowPattern,d)));
 219  *-------------------------------------------------------------------------
      -----*
 220   
 221   
 222  MODEL Model1 Find value of hydropower revenue using LP/ALL/;
 223  *This model is for all cases of steady flow days
 224   
 225  Loop((tot_vol,Nu_SteadyDays),
 226   
 227  TotMonth_volume = Vol_monthlyrelease(tot_vol);
 228  Num_Days(FlowType,Days)= Days_Distribution(FlowType,Days,Nu_SteadyDays)+ E
      PS;
 229  Energy_Price(Days,p)= Energy_Rate(Days,p);
 230   
 231  option LP= CPLEX;
 232   
 233  ******************************
 234  *initialization (Its optional in linear progromming but required in non-li
      near programming)
 235  *Release.L(FlowPattern,d,p)= 1000;
 236  **********************************
 237   
 238  Solve Model1 using LP MAXIMIZE ObjectiveVal;
 239   
 240  *-------------------------------------------------------------------------
      ---------------------------------------------
 241  *-------------------------------------------------------------------------
      ---------------------------------------------
 242  * All the following lines of code are saving values for different paramete
      rs
 243    FStore(tot_vol,Nu_SteadyDays)= ObjectiveVal.L;
 244   
 245  * XStore store the energy generated (Mwh/day) during different types of da
      ys
 246     XStore(tot_vol,Nu_SteadyDays,FlowType,Days)= sum (p,Energy_Gen.L(FlowTy
      pe,Days,p))+ EPS;
 247   
 248  *EngStore store total monthly energy generated from a number of steady day
      s scenario (Mwh per month)
 249     EngStore(tot_vol,Nu_SteadyDays)=   sum(FlowType, sum(Days, sum (p,Energ
      y_Gen.L(FlowType,Days,p))*Num_Days(FlowType,Days)))+ EPS;
 250   
 251  * RStore store the reservoir releases (cfs) during different types of days
       and scenarios.
 252    RStore(tot_vol,Nu_SteadyDays,FlowType,Days,p)= Release.L(FlowType,Days,p
      )+ EPS;
 253   
 254  *Sstore store the end of month reservoir storage (ac-ft)
 255    Sstore(tot_vol,Nu_SteadyDays)= storage.L;
 256   
 257   
 258  *Saving the model status for different scenarios.
 259     ModelResults(tot_vol,Nu_SteadyDays,"SolStat")= Model1.solvestat;
 260     ModelResults(tot_vol,Nu_SteadyDays,"ModStat")= Model1.modelstat;
 261   
 262   
 263     option clear=ObjectiveVal,clear=Release,clear=Energy_Gen;
 264  );
 265   
 266  DISPLAY FStore,XStore,RStore,Sstore,EngStore;
 267   
