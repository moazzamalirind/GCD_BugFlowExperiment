      ---------------------------------------------
 253  *-------------------------------------------------------------------------
      ---------------------------------------------
 254  * All the following lines of code are saving values for different paramete
      rs
 255   
 256     FStore(Offset,tot_vol,Nu_SteadyDays)= ObjectiveVal.L;
 257   
 258  * XStore store the energy generated (Mwh/day) during different types of da
      ys
 259     XStore(Offset,tot_vol,Nu_SteadyDays,FlowType,Days)= sum (p,Energy_Gen.L
      (FlowType,Days,p))+ EPS;
 260   
 261  * RStore store the reservoir releases (cfs) during different types of days
       and scenarios.
 262     RStore(Offset,tot_vol,Nu_SteadyDays,FlowType,Days,p)= Release.L(FlowTyp
      e,Days,p)+ EPS;
 263   
 264  *Sstore store the end of month reservoir storage (ac-ft)
 265     Sstore(Offset,tot_vol,Nu_SteadyDays)= storage.L;
 266   
 267   
 268  *Saving the model status for different scenarios.
 269     ModelResults(Offset,tot_vol,Nu_SteadyDays,"SolStat")= Model1.solvestat;
 270     ModelResults(Offset,tot_vol,Nu_SteadyDays,"ModStat")= Model1.modelstat;
 271   
 272   
 273     option clear=ObjectiveVal,clear=Release,clear=Energy_Gen;
 274  );
 275   
 276  DISPLAY FStore,XStore,RStore,Sstore;
