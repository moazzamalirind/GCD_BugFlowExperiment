 166  *-------------------------------------------------------------------------
      -----*
 167   
 168  *Mass Balance Equation
 169  EQ1_ResMassBal..                               storage =e= Initstorage + (
      Inflow*Convert*sum(p,Duration(p))*Totaldays)- Released_vol - evap;
 170  *                                                                         
       CFS * conversion factor from cfs to ac-ft/hr and multiplied by 24 (total 
      duration in a day) then multiplied by total day in the month
 171  *=======================Physical Constraints==============================
      =======
 172  EQ2_reqpowerstorage..                          storage =g= minstorage;
 173  EQ3_maxstor..                                  storage =l= maxstorage;
 174   
 175  EQ4_MaxR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=l= maxRel;
 176   
 177  EQ5_MinR(FlowPattern,d,p)$(Num_Days(FlowPattern,d) gt 0)..                
           Release(FlowPattern,d,p)=g= minRel;
 178   
 179  EQ6_ZeroFlowDays(FlowPattern,d,p)$(Num_Days(FlowPattern,d) eq 0)..        
           Release(FlowPattern,d,p)=e=0;
 180   
 181  EQ7_Rel_Range(FlowPattern,d)$(Num_Days(FlowPattern,d) gt 0)..             
           Release(FlowPattern,d,"pHigh")- Release(FlowPattern,d,"pLow")=l=Daily
      _RelRange;
 182   
 183  *EQ8_  constraining the overall monthly released volume..
 184  EQ8_Monthtlyrel..                                                         
           TotMonth_volume=e= Released_vol;
 185   
 186  EQ9_RelVolume..                                                           
           Released_vol=e= sum(FlowPattern,sum(d, sum(p, Release(FlowPattern,d,p
      )*Convert*Duration(p))*Num_Days(FlowPattern,d)));
 187   
 188  *======================Managerial Constraints=============================
      ======================
 189   
 190  EQ10_SteadyFlow_Day(FlowPattern,d)$(Num_Days("Steady",d) gt 0)..          
           Release("Steady",d,"pHigh") =e= Release("Steady",d,"pLow");
 191   
 192  EQ11_Unsteadydays_OffpeakRel(FlowPattern,d)$(Num_Days("Unsteady",d) gt 0).
      .    Release("Unsteady",d,"pLow")=e= Release("Unsteady","Weekday","pLow");
 193   
 194   
 195  EQ12_OffsetRel(FlowPattern)$((Num_days("Unsteady","Weekday") gt 0) and (Nu
      m_Days("Steady","Sunday") gt 0))..          Release("Steady","Sunday","pLo
      w")=e= Release("Unsteady","Weekday","pLow")+ Weekend_Rel;
 196  * EQ12_  finds the minimimum release value from the hydrograph plus additi
      onal release we desire on weekends above off-peak weekday release (i.e. Of
      fset).
 197   
 198  EQ13_Zero_OffsetRel(FlowPattern,d,p)$(Num_Days("Steady","Sunday") eq 0).. 
           Release("Unsteady",d,p)=e= Release("Unsteady","Weekday",p);
 199   
 200  EQ14_Steady_Saturdays(FlowPattern,d,p)$(Num_Days("Steady","Saturday")gt 0)
      ..   Release("Steady","Saturday",p)=e= Release("Steady","Sunday",p);
 201   
 202  EQ15_Steady_Weekdays(FlowPattern,d,p)$(Num_Days("Steady","Weekday")gt 0)..
           Release("Steady","Weekday",p)=e= Release("Steady","Sunday",p);
 203   
 204  *EQ16_Steady_Weekdays(FlowPattern,d,p)$((Num_Days("Steady","Weekday")gt 0)
      and(Num_Days("Unsteady","Weekday")gt 0))..     Release("Steady","Weekday",
      p)=e= Release("Steady","Sunday",p)- Weekend_Rel ;
 205   
 206   
 207  EQ17_OnPeak_UnsteadyDay(FlowPattern,d)$(Num_Days("Unsteady",d)gt 0)..     
           Release("Unsteady",d,"pHigh")=g= Release("Unsteady",d,"pLow");
 208   
 209  *==============================================================
 210  *EQ18a_OnPeakUnsteady_Sat(FlowPattern,d)$((Num_Days("Unsteady","Saturday")
      gt 0)and (Num_Days("Steady","Sunday") gt 0))..                  Release("U
      nsteady","Saturday","pHigh")=g= 11000;
 211  *EQ18b_OnPeakUnsteady_Sun(FlowPattern,d)$((Num_Days("Unsteady","Sunday")gt
       0)and (Num_Days("Steady","Sunday") gt 0))..                    Release("U
      nsteady","Sunday","pHigh")=g= 10000;
 212  *==============================================================
 213   
 214  EQ19_EnergyGen_Max(FlowPattern,d,p)..                                     
           Energy_Gen(FlowPattern,d,p)=l= 1320*Duration(p);
 215  *Maximum Energy Generation capacity of GCD (MWH).. Source https://www.usbr
      .gov/uc/rm/crsp/gc
 216  EQ20_EnergyGen(FlowPattern,d,p)..                                         
           Energy_Gen(FlowPattern,d,p)=e= Release(FlowPattern,d,p)*Duration(p)*0
      .03715;
 217  * Energy generation formula used in WAPA Execl model..
 218   
 219  *==============================Objective Function=========================
      =============
 220  EQ21_EnergyRevenue..                                                      
           ObjectiveVal=e= sum(FlowPattern,sum(d, sum(p,Energy_Gen(FlowPattern,d
      ,p)*Energy_Price(d,p))*Num_Days(FlowPattern,d)));
 221  *-------------------------------------------------------------------------
      -----*
 222   
 223   
 224   
 225  MODEL Model1 Find value of hydropower revenue using LP/ALL/;
 226  *This model is for all cases of steady flow days
 227   
 228  Loop((Offset,tot_vol,Nu_SteadyDays),
 229   
 230  Weekend_Rel= Diff_Release(Offset);
 231  TotMonth_volume = Vol_monthlyrelease(tot_vol);
 232  Num_Days(FlowType,Days)= Days_Distribution(FlowType,Days,Nu_SteadyDays)+ E
      PS;
 233  Energy_Price(Days,p)= Energy_Rate(Days,p);
 234   
 235  option LP= CPLEX;
 236   
 237  ******************************
 238  *initialization (Its optional in linear progromming but required in non-li
      near programming)
 239  *Release.L(FlowPattern,d,p)= 1000;
 240  **********************************
 241   
 242  Solve Model1 using LP MAXIMIZE ObjectiveVal;
 243   
 244  *-------------------------------------------------------------------------
      ---------------------------------------------
 245  *-------------------------------------------------------------------------
      ---------------------------------------------
 246  * All the following lines of code are saving values for different paramete
      rs
 247   
 248     FStore(Offset,tot_vol,Nu_SteadyDays)= ObjectiveVal.L;
 249   
 250  * XStore store the energy generated (Mwh/day) during different types of da
      ys
 251     XStore(Offset,tot_vol,Nu_SteadyDays,FlowType,Days)= sum (p,Energy_Gen.L
      (FlowType,Days,p))+ EPS;
 252   
 253  * RStore store the reservoir releases (cfs) during different types of days
       and scenarios.
 254     RStore(Offset,tot_vol,Nu_SteadyDays,FlowType,Days,p)= Release.L(FlowTyp
      e,Days,p)+ EPS;
 255   
 256  *Sstore store the end of month reservoir storage (ac-ft)
 257     Sstore(Offset,tot_vol,Nu_SteadyDays)= storage.L;
 258   
 259   
 260  *Saving the model status for different scenarios.
 261     ModelResults(Offset,tot_vol,Nu_SteadyDays,"SolStat")= Model1.solvestat;
 262     ModelResults(Offset,tot_vol,Nu_SteadyDays,"ModStat")= Model1.modelstat;
 263   
 264   
 265     option clear=ObjectiveVal,clear=Release,clear=Energy_Gen;
 266  );
 267   
 268  DISPLAY FStore,XStore,RStore,Sstore;
