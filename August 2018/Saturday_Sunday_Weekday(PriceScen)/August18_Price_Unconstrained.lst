      ---------------------------------------------
 245  *-------------------------------------------------------------------------
      ---------------------------------------------
 246  * All the following lines of code are saving values for different paramete
      rs
 247   
 248     FStore(Offset,tot_vol,Nu_SteadyDays)= ObjectiveVal.L;
 249   
 250  * XStore store the energy generated (Mwh/day) during different types of da
      ys
 251     XStore(Offset,tot_vol,Nu_SteadyDays,FlowType,Days)= sum (p,Energy_Gen.L
      (FlowType,Days,p))+ EPS;
 252   
 253  * RStore store the reservoir releases (cfs) during different types of days
       and scenarios.
 254     RStore(Offset,tot_vol,Nu_SteadyDays,FlowType,Days,p)= Release.L(FlowTyp
      e,Days,p)+ EPS;
 255   
 256  *Sstore store the end of month reservoir storage (ac-ft)
 257     Sstore(Offset,tot_vol,Nu_SteadyDays)= storage.L;
 258   
 259   
 260  *Saving the model status for different scenarios.
 261     ModelResults(Offset,tot_vol,Nu_SteadyDays,"SolStat")= Model1.solvestat;
 262     ModelResults(Offset,tot_vol,Nu_SteadyDays,"ModStat")= Model1.modelstat;
 263   
 264   
 265     option clear=ObjectiveVal,clear=Release,clear=Energy_Gen;
 266  );
 267   
 268  DISPLAY FStore,XStore,RStore,Sstore;
